<!--Ids are used in identifying the boosts, increment ids linearly,
    do not modify Ids, or add in between-->
<!--follow the number format rules while specifying number, can be given
    in any format-->

<BoostConfigs>
    <PerfBoost>
    <!--app lauch boost-->
        <!-- SCHEDBOOST resource opcode, value-->
        <!-- CPUBOOST_MIN_FREQ BIG Core resource opcode, value-->
        <!-- DISABLE_POWER_COLLAPSE resource opcode, value-->
        <!-- CLUSTR_0_CPUS_ON resource opcode, value-->
        <!-- GPU_MIN_PWRLVL resource opcode, value-->

        <!-- Type="1", main launch boost of 2sec -->
        <Config
            Id="0x00001081" Type="1" Enable="true" Timeout="2000" Target="msm8952"
            Resources="0x40C00000, 0x1, 0x40800000, 0x70C, 0x40400000, 0x1, 0x41000000, 0x4, 0x42804000, 0x1" />

        <Config
            Id="0x00001081" Type="1" Enable="true" Timeout="2000" Target="msm8956"
            Resources="0x40C00000, 0x1, 0x40800000, 0x70C, 0x40400000, 0x1, 0x41000000, 0x4, 0x42804000, 0x1" />

        <Config
            Id="0x00001081" Type="1" Enable="true" Timeout="2000" Target="msm8976"
            Resources="0x40C00000, 0x1, 0x40800000, 0x70C, 0x40400000, 0x1, 0x41000000, 0x4, 0x42804000, 0x1" />

    <!--app lauch boost (disabling packing)-->
        <!-- SCHED_PREFER_IDLE resource opcode, value-->
        <!-- SCHED_SMALL_TASK resource opcode, value-->
        <!-- SCHED_IDLE_NR_RUN resource opcode, value-->
        <!-- SCHED_IDLE_LOAD resource opcode, value-->

        <!-- Type="2", launch boost for disable packing 1.5sec -->
        <Config
            Id="0x00001081" Type="2" Enable="true" Timeout="1500" Target="msm8952"
            Resources="0x40C04000, 0x1, 0x40C0C000, 0x1, 0x40C14000, 0x1, 0x40C10000, 0x1" />

        <Config
            Id="0x00001081" Type="2" Enable="true" Timeout="1500" Target="msm8956"
            Resources="0x40C04000, 0x1, 0x40C0C000, 0x1, 0x40C14000, 0x1, 0x40C10000, 0x1" />

        <Config
            Id="0x00001081" Type="2" Enable="true" Timeout="1500" Target="msm8976"
            Resources="0x40C04000, 0x1, 0x40C0C000, 0x1, 0x40C14000, 0x1, 0x40C10000, 0x1" />
            
    <!--resume boost-->
        <!-- SCHEDBOOST resource opcode, value-->
        <!-- CPUBOOST_MAX_FREQ BIG Core resource opcode, value-->
        <!-- CPUBOOST_MAX_FREQ LITTLE Core resource opcode, value-->
        <!-- CPUBOOST_MIN_FREQ BIG Core resource opcode, value-->
        <!-- CPUBOOST_MIN_FREQ LITTLE Core resource opcode, value-->
        <!-- POWER COLLAPSE resource opcode, value-->
        <!-- STORAGE CLK SCALING resource opcode, value-->
        <!-- GPU DISABLE resource opcode, value-->

        <Config
            Id="0x00001082" Type="1" Enable="true" Timeout="400" Target="msm8952"
            Resources="0x40C00000, 0x1, 0x40804000, 0x70C, 0x40804100, 0x579, 0x40800000, 0x70C,
                       0x40800100, 0x579, 0x40400000, 0x1, 0x42C10000, 0x1, 0x4281C000, 0x1" />
        
        <Config
            Id="0x00001082" Type="1" Enable="true" Timeout="400" Target="msm8956"
            Resources="0x40C00000, 0x1, 0x40804000, 0x70C, 0x40804100, 0x579, 0x40800000, 0x70C,
                       0x40800100, 0x579, 0x40400000, 0x1, 0x42C10000, 0x1, 0x4281C000, 0x1" />
        
        <Config
            Id="0x00001082" Type="1" Enable="true" Timeout="400" Target="msm8976"
            Resources="0x40C00000, 0x1, 0x40804000, 0x70C, 0x40804100, 0x579, 0x40800000, 0x70C,
                       0x40800100, 0x579, 0x40400000, 0x1, 0x42C10000, 0x1, 0x4281C000, 0x1" />

    <!-- config_enablePerfBoostForAnimation-->
        <!-- SCHEDBOOST resource opcode, value-->
        <!-- CPUBOOST_MIN_FREQ BIG Core resource opcode, value-->
        <!-- GPU_MIN_PWRLVL resource opcode, value-->

        <!-- Type="1", Animation boost of 0.6sec -->
        <Config
            Id="0x00001083" Enable="true" Timeout="600" Target="msm8952"
            Resources="0x40C00000, 0x1, 0x40800000, 0x420, 0x42804000, 0x1" />

        <Config
            Id="0x00001083" Enable="true" Timeout="600" Target="msm8956"
            Resources="0x40C00000, 0x1, 0x40800000, 0x420, 0x42804000, 0x1" />

        <Config
            Id="0x00001083" Enable="true" Timeout="600" Target="msm8976"
            Resources="0x40C00000, 0x1, 0x40800000, 0x420, 0x42804000, 0x1" />

    <!--Vertical Scroll boost-->
        <!-- SCHEDBOOST resource opcode, value-->
        <!-- CPUBOOST_MIN_FREQ BIG Core resource opcode, value-->
        <!-- SCHED_PREFER_IDLE resource opcode, value-->

        <!-- Type="1", Vertical Scroll boost -->
        <Config
            Id="0x00001080" Type="1" Enable="true" Timeout="600" Target="msm8952"
            Resources="0x40C00000, 0x1, 0x40800000, 0x420, 0x40C04000, 0x1" />

        <Config
            Id="0x00001080" Type="1" Enable="true" Timeout="600" Target="msm8956"
            Resources="0x40C00000, 0x1, 0x40800000, 0x420, 0x40C04000, 0x1" />

        <Config
            Id="0x00001080" Type="1" Enable="true" Timeout="600" Target="msm8976"
            Resources="0x40C00000, 0x1, 0x40800000, 0x420, 0x40C04000, 0x1" />

    <!--Horizontal Scroll boost-->
        <!-- SCHEDBOOST resource opcode, value-->
        <!-- CPUBOOST_MIN_FREQ BIG Core resource opcode, value-->
        <!-- SCHED_PREFER_IDLE resource opcode, value-->

        <!-- Type="2", Horizontal Scroll boost -->
        <Config
            Id="0x00001080" Type="2" Enable="true" Timeout="600" Target="msm8952"
            Resources="0x40C00000, 0x1, 0x40800000, 0x420, 0x40C04000, 0x1" />

        <Config
            Id="0x00001080" Type="2" Enable="true" Timeout="600" Target="msm8956"
            Resources="0x40C00000, 0x1, 0x40800000, 0x420, 0x40C04000, 0x1" />

        <Config
            Id="0x00001080" Type="2" Enable="true" Timeout="600" Target="msm8976"
            Resources="0x40C00000, 0x1, 0x40800000, 0x420, 0x40C04000, 0x1" />
            
    <!--Webview Scroll boost-->
        <!-- CPUBOOST_MIN_FREQ LITTLE Core resource opcode, value -->
        <!-- CPUBOOST_MIN_FREQ BIG CORE resource opcode, value -->
        <!-- SCHEDBOOST resource opcode, value-->
        <!-- Type="5", webview Scroll boost -->
        <Config
            Id="0x00001080" Type="5" Enable="true" Timeout="2500" Target="msm8952"
            Resources="0x40800100, 0x3F9, 0x40800000, 0x459,0x40C00000, 0x2" />
            
        <Config
            Id="0x00001080" Type="5" Enable="true" Timeout="2500" Target="msm8956"
            Resources="0x40800100, 0x3F9, 0x40800000, 0x459,0x40C00000, 0x2" />
        
        <Config
            Id="0x00001080" Type="5" Enable="true" Timeout="2500" Target="msm8976"
            Resources="0x40800100, 0x3F9, 0x40800000, 0x459,0x40C00000, 0x2" />
  
    <!--Drag boost-->
        <!-- CPUBOOST_MIN_FREQ LITTLE Core resource opcode, value -->
        <!-- CPUBOOST_MIN_FREQ BIG CORE resource opcode, value -->
        <!-- SCHEDBOOST resource opcode, value-->
        <!-- Type="1", Drag boost -->
        <Config
            Id="0x00001087" Type="1" Enable="true" Timeout="0" Target="msm8952"
            Resources="0x40800100, 0x326, 0x40800000, 0x420,0x40C00000, 0x2" />
            
        <Config
            Id="0x00001087" Type="1" Enable="true" Timeout="0" Target="msm8956"
            Resources="0x40800100, 0x326, 0x40800000, 0x420,0x40C00000, 0x2" />
            
        <Config
            Id="0x00001087" Type="1" Enable="true" Timeout="0" Target="msm8976"
            Resources="0x40800100, 0x326, 0x40800000, 0x420,0x40C00000, 0x2" />

    <!--PackageInstall boost-->
        <!-- SCHEDBOOST resource opcode, value-->
        <!-- CPUBOOST_MAX_FREQ BIG Core resource opcode, value-->
        <!-- CPUBOOST_MAX_FREQ LITTLE Core resource opcode, value-->
        <!-- CPUBOOST_MIN_FREQ BIG Core resource opcode, value-->
        <!-- CPUBOOST_MIN_FREQ LITTLE Core resource opcode, value-->
        <!-- Type="", Package Install boost -->

        <Config
            Id="0x00001088" Enable="true" Target="msm8952"
            Resources="0x40C00000, 0x1, 0x40804000, 0x70C, 0x40804100, 0x579,
                                        0x40800000, 0x519, 0x40800100, 0x519" />
                       
        <Config
            Id="0x00001088" Enable="true" Target="msm8956"
            Resources="0x40C00000, 0x1, 0x40804000, 0x70C, 0x40804100, 0x579,
                                        0x40800000, 0x519, 0x40800100, 0x519" />
                       
        <Config
            Id="0x00001088" Enable="true" Target="msm8976"
            Resources="0x40C00000, 0x1, 0x40804000, 0x70C, 0x40804100, 0x579,
                                        0x40800000, 0x519, 0x40800100, 0x519" />
    
    </PerfBoost>
</BoostConfigs>
